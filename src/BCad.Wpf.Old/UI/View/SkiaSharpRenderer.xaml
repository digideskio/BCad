<local:AbstractCadRenderer x:Class="IxMilia.BCad.UI.View.SkiaSharpRenderer"
                           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                           xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                           xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                           xmlns:local="clr-namespace:IxMilia.BCad.UI.View"
                           xmlns:skia="clr-namespace:SkiaSharp.Views.WPF;assembly=SkiaSharp.Views.WPF"
                           mc:Ignorable="d"
                           Loaded="RendererLoaded"
                           d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <skia:SKElement x:Name="element" PaintSurface="PaintSurface" />
    </Grid>
</local:AbstractCadRenderer>
